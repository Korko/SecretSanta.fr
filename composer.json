{
    "name": "korko/secretsanta",
    "description": "An organsation tool for secret santa parties.",
    "keywords": [
        "secretsanta",
        "tool"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.3|^8.0",
        "anhskohbo/no-captcha": "^3.3",
        "beyondcode/laravel-websockets": "^1.9",
        "fideloper/proxy": "^4.4",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "korko/laravel-dkim": "dev-master",
        "korko/validator": "dev-master",
        "laravel/framework": "^8.18",
        "laravel/tinker": "^2.5",
        "league/statsd": "1.5.*",
        "pusher/pusher-php-server": "^4.1",
        "spatie/laravel-blade-javascript": "^2.5",
        "vinkla/hashids": "^9.0",
        "webklex/laravel-imap": "^2.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.5",
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "laravel/sail": "^1.0.1",
        "matt-allan/laravel-code-style": "dev-master",
        "martinlindhe/laravel-vue-i18n-generator": "dev-master",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.3.3"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "check-style": "php-cs-fixer fix --dry-run --allow-risky=yes --diff",
        "fix-style": "php-cs-fixer fix --allow-risky=yes"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/korko/laravel-dkim"
        },
        {
            "type": "vcs",
            "url": "https://github.com/korko/laravel-vue-i18n-generator"
        }
    ]
}
