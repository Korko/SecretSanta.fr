{
    "name": "korko/secretsanta",
    "description": "An organsation tool for secret santa parties.",
    "keywords": ["secretsanta", "tool"],
    "license": "MIT",
    "type": "project",
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "korko/laravel5-recaptcha2",
                "version": "dev-Korko-patch-1",
                "source": {
                    "url": "https://github.com/korko/laravel5-recaptcha2.git",
                    "type": "git",
                    "reference": "Korko-patch-1"
                },
                "autoload": {
                    "classmap": [""]
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "korko/statsd",
                "version": "dev-laravel-5.4",
                "source": {
                    "url": "https://github.com/korko/statsd.git",
                    "type": "git",
                    "reference": "laravel-5.4"
                },
                "autoload": {
                    "classmap": [""]
                }
            }
        }
    ],
    "require": {
        "php": "^7.0",
        "korko/callr": "^1.0",
        "korko/laravel5-recaptcha2": "dev-Korko-patch-1",
        "korko/statsd": "dev-laravel-5.4",
        "korko/validator": "dev-master",
        "laravel/framework": "5.4.*",
        "mariuzzo/laravel-js-localization": "^1.4",
        "spatie/laravel-blade-javascript": "^1.0"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "laravel/tinker": "~1.0",
        "mockery/mockery": "0.9.*",
        "jakub-onderka/php-parallel-lint": "^1.0",
        "phpro/grumphp": "^0.14.1",
        "phpunit/phpunit": "~5.0",
        "povils/phpmnd": "^2.0",
        "rap2hpoutre/laravel-log-viewer": "^0.8.0",
        "squizlabs/PHP_CodeSniffer": "^3.2",
        "symfony/css-selector": "3.2.*",
        "symfony/dom-crawler": "3.2.*"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "Korko\\SecretSanta\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    }
}
