{
    "name": "korko/secretsanta",
    "description": "An organsation tool for secret santa parties.",
    "keywords": [
        "secretsanta",
        "tool"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^8.1",
        "beyondcode/laravel-websockets": "^1.14",
        "doctrine/dbal": "^3.5",
        "elhebert/laravel-sri": "dev-master",
        "exussum12/xxhash": "^1.3",
        "guzzlehttp/guzzle": "^7.2",
        "inertiajs/inertia-laravel": "^0.6",
        "korko/validator": "dev-master",
        "laravel/dusk": "^7.4",
        "laravel/framework": "^10.2",
        "laravel/tinker": "^2.8",
        "lexo/defer-vite": "^1.0",
        "predis/predis": "^2.0",
        "pusher/pusher-php-server": "^7.0",
        "spatie/laravel-blade-javascript": "^2.7",
        "stechstudio/laravel-metrics": "^1.5",
        "tightenco/ziggy": "^1.5",
        "vinkla/hashids": "^11.0",
        "webklex/laravel-imap": "^2.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.8",
        "barryvdh/laravel-ide-helper": "^2.13",
        "fakerphp/faker": "^1.9.1",
        "laravel/pint": "^1.2",
        "laravel/sail": "^1.18",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^6.1",
        "pestphp/pest-plugin-faker": "^1.0",
        "pestphp/pest-plugin-global-assertions": "^1.0",
        "pestphp/pest-plugin-laravel": "^1.4",
        "pestphp/pest-plugin-parallel": "^1.0",
        "phpunit/phpunit": "^9.5.10",
        "spatie/laravel-ignition": "^2.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force",
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "check-style": "php-cs-fixer fix --dry-run --allow-risky=yes --diff",
        "fix-style": "php-cs-fixer fix --allow-risky=yes"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/korko/laravel-vue-i18n-generator"
        }
    ]
}
